<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">

<sqlMap namespace="Comtnemplyrscrtyestbs">
	
	<typeAlias  alias="egovMap" type="egovframework.rte.psl.dataaccess.util.EgovMap"/>
	<typeAlias  alias="ComtnemplyrscrtyestbsVO" type="kr.godsoft.egovframe.generatorwebapp.comtnemplyrscrtyestbs.service.ComtnemplyrscrtyestbsVO"/>
	
	<resultMap id="comtnemplyrscrtyestbs" class="kr.godsoft.egovframe.generatorwebapp.comtnemplyrscrtyestbs.service.ComtnemplyrscrtyestbsVO">
		<result property="scrtyDtrmnTrgetId" column="scrty_dtrmn_trget_id" columnIndex="1"/>
		<result property="mberTyCode" column="mber_ty_code" columnIndex="2"/>
		<result property="authorCode" column="author_code" columnIndex="3"/>
	</resultMap>
	
	<insert id="comtnemplyrscrtyestbsDAO.insertComtnemplyrscrtyestbs_S">
		<![CDATA[
            INSERT INTO comtnemplyrscrtyestbs (
                scrty_dtrmn_trget_id
                , mber_ty_code
                , author_code
            ) VALUES (
                #scrtyDtrmnTrgetId#
                , #mberTyCode#
                , #authorCode#
            )
		]]>
	</insert>
	
	<update id="comtnemplyrscrtyestbsDAO.updateComtnemplyrscrtyestbs_S">
		<![CDATA[
            UPDATE comtnemplyrscrtyestbs
                SET
                scrty_dtrmn_trget_id = #scrtyDtrmnTrgetId#
                , mber_ty_code = #mberTyCode#
                , author_code = #authorCode#
                WHERE 1 = 1

		]]>
	</update>
	
	<delete id="comtnemplyrscrtyestbsDAO.deleteComtnemplyrscrtyestbs_S">
		<![CDATA[
			DELETE FROM comtnemplyrscrtyestbs
				WHERE 1 = 1
		]]>
	</delete>
	
	<select id="comtnemplyrscrtyestbsDAO.selectComtnemplyrscrtyestbs_S" resultMap="comtnemplyrscrtyestbs">
		<![CDATA[
			SELECT
				scrty_dtrmn_trget_id
				, mber_ty_code
				, author_code
			FROM COMTNEMPLYRSCRTYESTBS
		]]>
	</select>
	
	<select id="comtnemplyrscrtyestbsDAO.selectComtnemplyrscrtyestbsList_D" parameterClass="ComtnemplyrscrtyestbsVO" resultClass="egovMap">
	       SELECT 
								A.SCRTY_DTRMN_TRGET_ID
								, A.MBER_TY_CODE
								, A.AUTHOR_CODE
			       FROM
            (
				SELECT
								SCRTY_DTRMN_TRGET_ID
								, MBER_TY_CODE
								, AUTHOR_CODE
							, ROWNUM
				FROM COMTNEMPLYRSCRTYESTBS
				WHERE 1=1
				<isEqual prepend="AND" property="searchCondition" compareValue="0">
					SCRTY_DTRMN_TRGET_ID = #searchKeyword#
				</isEqual>
					<isEqual prepend="AND" property="searchCondition" compareValue="1">
					MBER_TY_CODE LIKE '%' || #searchKeyword# || '%'
				</isEqual>
						ORDER BY 
					) A
			WHERE ROWNUM BETWEEN #firstIndex# AND #recordCountPerPage#
	</select>

	<select id="comtnemplyrscrtyestbsDAO.selectComtnemplyrscrtyestbsListTotCnt_S" parameterClass="ComtnemplyrscrtyestbsVO" resultClass="int">
			SELECT COUNT(*) totcnt
			FROM COMTNEMPLYRSCRTYESTBS
			WHERE 1=1
			<isEqual prepend="AND" property="searchCondition" compareValue="0">
				SCRTY_DTRMN_TRGET_ID = #searchKeyword#
			</isEqual>
				<isEqual prepend="AND" property="searchCondition" compareValue="1">
				MBER_TY_CODE LIKE '%' || #searchKeyword# || '%'
			</isEqual>
		</select>

