<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">

<sqlMap namespace="Comtecopseq">
	
	<typeAlias  alias="egovMap" type="egovframework.rte.psl.dataaccess.util.EgovMap"/>
	<typeAlias  alias="ComtecopseqVO" type="kr.godsoft.egovframe.generatorwebapp.comtecopseq.service.ComtecopseqVO"/>
	
	<resultMap id="comtecopseq" class="kr.godsoft.egovframe.generatorwebapp.comtecopseq.service.ComtecopseqVO">
		<result property="tableName" column="table_name" columnIndex="1"/>
		<result property="nextId" column="next_id" columnIndex="2"/>
	</resultMap>
	
	<insert id="comtecopseqDAO.insertComtecopseq_S">
		<![CDATA[
            INSERT INTO comtecopseq (
                table_name
                , next_id
            ) VALUES (
                #tableName#
                , #nextId#
            )
		]]>
	</insert>
	
	<update id="comtecopseqDAO.updateComtecopseq_S">
		<![CDATA[
            UPDATE comtecopseq
                SET
                table_name = #tableName#
                , next_id = #nextId#
                WHERE 1 = 1
                AND table_name = #tableName#
		]]>
	</update>
	
	<delete id="comtecopseqDAO.deleteComtecopseq_S">
		<![CDATA[
			DELETE FROM comtecopseq
				WHERE 1 = 1
				AND table_name = #tableName#
		]]>
	</delete>
	
	<select id="comtecopseqDAO.selectComtecopseq_S" resultMap="comtecopseq">
		<![CDATA[
			SELECT
				table_name
				, next_id
			FROM COMTECOPSEQ
			WHERE table_name = #tableName#
		]]>
	</select>
	
	<select id="comtecopseqDAO.selectComtecopseqList_D" parameterClass="ComtecopseqVO" resultClass="egovMap">
	       SELECT 
								A.TABLE_NAME
								, A.NEXT_ID
			       FROM
            (
				SELECT
								TABLE_NAME
								, NEXT_ID
							, ROWNUM
				FROM COMTECOPSEQ
				WHERE 1=1
				<isEqual prepend="AND" property="searchCondition" compareValue="0">
					TABLE_NAME = #searchKeyword#
				</isEqual>
					<isEqual prepend="AND" property="searchCondition" compareValue="1">
					NEXT_ID LIKE '%' || #searchKeyword# || '%'
				</isEqual>
						ORDER BY 
							TABLE_NAME DESC
					) A
			WHERE ROWNUM BETWEEN #firstIndex# AND #recordCountPerPage#
	</select>

	<select id="comtecopseqDAO.selectComtecopseqListTotCnt_S" parameterClass="ComtecopseqVO" resultClass="int">
			SELECT COUNT(*) totcnt
			FROM COMTECOPSEQ
			WHERE 1=1
			<isEqual prepend="AND" property="searchCondition" compareValue="0">
				TABLE_NAME = #searchKeyword#
			</isEqual>
				<isEqual prepend="AND" property="searchCondition" compareValue="1">
				NEXT_ID LIKE '%' || #searchKeyword# || '%'
			</isEqual>
		</select>

</sqlMap>
